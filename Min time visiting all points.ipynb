    "def minTimeToVisitAllPoints(a):\n",
    "    time=0\n",
    "    for i in range(1, len(a)):\n",
    "        b=abs(a[i][0]-a[i-1][0])\n",
    "        c=abs(a[i][1]-a[i-1][1])\n",
    "        time+= max(b,c)\n",
    "    return time\n",
    "a=[[1,1],[3,4],[-1,0]]\n",
    "d=minTimeToVisitAllPoints(a)\n",
    "print(d)"
   ]
